<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.6</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.0.0</Version>
    <Authors>Getty Images</Authors>
    <Company>Getty Images</Company>
    <Product>Getty Images API SDK</Product>
    <Title>Getty Images API SDK</Title>
    <Description>Allows easy integration with the Getty Images API.</Description>
    <Copyright>Copyright Â© 2014 - 2018 Getty Images</Copyright>
    <PackageTags>Getty Images Videos API SDK GettyImages Photo Photos Search Creative Editorial Artist Embed</PackageTags>
    <PackageLicenseUrl>https://github.com/gettyimages/gettyimages-api_dotnet/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/gettyimages/gettyimages-api_dotnet</PackageProjectUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/gettyimages/gettyimages-api_dotnet/master/package-icon.jpg</PackageIconUrl>
    <RepositoryUrl>https://github.com/gettyimages/gettyimages-api_dotnet</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>

</Project>
